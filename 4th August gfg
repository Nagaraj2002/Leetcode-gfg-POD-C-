//User function Template for C++

class Solution{
public:
    void Reverse(stack<int> &St){
        vector<int>v;
        while(!St.empty()){
            int a= St.top();
            St.pop();
            v.push_back(a);
        }
        for(int i=0;i<v.size();i++){
            cout<<v[i]<<" ";
        }
    }
};
